{
  "version": 2,
  "name": "mosqit",
  "alias": ["mosqit.vercel.app"],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/dashboard",
      "dest": "/dashboard"
    },
    {
      "src": "/logs",
      "dest": "/logs"
    },
    {
      "src": "/issues",
      "dest": "/issues"
    },
    {
      "src": "/settings",
      "dest": "/settings"
    },
    {
      "src": "/api/extension/(.*)",
      "dest": "/api/extension/$1"
    },
    {
      "src": "/api/github/(.*)",
      "dest": "/api/github/$1"
    }
  ],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1",
    "SKIP_ENV_VALIDATION": "1"
  },
  "build": {
    "env": {
      "VERCEL": "1"
    }
  }
}