{
  "name": "mosqit",
  "version": "1.0.0",
  "private": true,
  "author": "Mosqit Team",
  "description": "AI-powered frontend debugging Chrome Extension with DevTools integration",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/mosqit.git"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "echo \"Tests will be added\" && exit 0",
    "test:unit": "jest --coverage",
    "test:e2e": "playwright test",
    "analyze": "ANALYZE=true next build",
    "build:extension": "node scripts/build-extension.js",
    "build:extension:prod": "NODE_ENV=production node scripts/build-extension.js",
    "build:dashboard": "next build",
    "build:all": "npm run build:dashboard && npm run build:extension",
    "dev:extension": "node scripts/watch-extension.js",
    "package:extension": "node scripts/package-extension.js",
    "setup": "node scripts/setup.js",
    "setup:validate": "node scripts/setup.js validate",
    "setup:test": "node scripts/setup.js test",
    "deploy:preview": "vercel",
    "deploy:production": "vercel --prod",
    "env:check": "node scripts/check-env.js",
    "clean": "rm -rf .next dist node_modules/.cache",
    "clean:all": "rm -rf .next dist node_modules package-lock.json"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "next": "15.5.3"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "@eslint/eslintrc": "^3"
  }
}
